{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.jpeg",
          "**/*.gif",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "author": "davidbritch",
      "breadcrumb_path": "/dotnet/maui/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "GitHub",
      "feedback_github_repo": "dotnet/docs-maui",
      "ms.author": "dabritch",
      "ms.prod": "dotnet-mobile",
      "ms.technology": "dotnet-maui",
      "zone_pivot_group_filename": "maui/zone-pivot-groups.json",
      "titleSuffix": ".NET MAUI"
    },
    "fileMetadata": {
        "titleSuffix": {
            "essentials/**.md": ".NET MAUI Essentials"
        }
    },
    "template": [],
    "dest": "docs-maui-public",
    "markdownEngineName": "markdig"
  }
}
